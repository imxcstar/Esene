    GetText2 = BytesToBstr(a)
    st = 0
    Exit Function
Err:
    st = -1
    GetText2 = ""
End Function

Public Function GetText(Optional st As Integer) As String
    On Error GoTo Err
    Dim zc As String, ZC2 As String
    zc = Clipboard.GetText
    Clipboard.Clear
    SendEditor SCI_COPYRANGE, 0, Length
    ZC2 = Clipboard.GetText
    Clipboard.SetText zc
    GetText = ZC2
    st = 0
    Exit Function
Err:
    GetText = GetText2(st)
End Function

Public Function GetEncText() As String
    Dim a() As Byte
    Dim B As Long
    Dim JB As Long
    JB = GetHwnd
    B = SendMessageW(JB, &HE, 0, 0)
    If B = 0 Then Exit Function
    ReDim a(0 To B - 1) As Byte
    Call SendMessageW(JB, &HD, ByVal B + 1, a(0))
    Dim i As Integer
    Dim zc As String
    For i = 0 To UBound(a)
       zc = zc & "%" & Hex(a(i))
    Next i
    GetEncText = zc
End Function

Public Sub ToClipboard(Optional start As Long = 0, Optional eend As Long = 0)
    If start = 0 And eend = 0 Then
        On Error Resume Next
        Clipboard.Clear
        SendEditor SCI_COPYRANGE, 0, Length
    Else
        SendEditor SCI_COPYRANGE, start, eend
    End If
End Sub

Public Sub Move(Left As Long, Top As Long, Width As Long, Height As Long)
    SetWindowPos sci, 0, Left / Screen.TwipsPerPixelX, Top / Screen.TwipsPerPixelY, Width / Screen.TwipsPerPixelX, Height / Screen.TwipsPerPixelY, 0
End Sub

Private Function SendEditor(a As Long, Optional B As Long = 0, Optional C As Long = 0) As Long
    SendEditor = SendMessage(sci, a, B, C)
End Function

Public Function GetHwnd() As Long
    GetHwnd = sci
End Function

Public Sub SaveTFile(FileName As String)
    Dim zc As String
    Dim st As Integer
    zc = GetText(st)
    If st = 0 Then
        Dim TempFile As Long
        TempFile = FreeFile
        Open FileName For Output As #TempFile
        Print #TempFile, zc
        Close #TempFile
    End If
End Sub

Public Sub OpenTFile(FileName As String)
    Dim TempFile As Long
    Dim LoadBytes() As Byte
    TempFile = FreeFile
    Open FileName For Binary As #TempFile
    ReDim LoadBytes(0 To LOF(TempFile) - 1) As Byte
    Get #TempFile, , LoadBytes
    Close TempFile
    SetText StrConv(LoadBytes, vbUnicode)
End Sub

