Public Function GetDqwz() As Long
    GetDqwz = SendEditor(SCI_GETCURRENTPOS)
End Function

Public Function GetDqText() As String
    Dim q As Long, h As Long
    q = SendEditor(SCI_WORDSTARTPOSITION, GetDqwz - 1)
    h = SendEditor(SCI_WORDENDPOSITION, GetDqwz - 1)
    If q = 0 Or h = 0 Then GetDqText = "": Exit Function
    GetDqText = Replace(Replace(Mid(GetText, q, (h - q) + 1), " ", ""), vbTab, "")
End Function

